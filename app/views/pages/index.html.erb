<%= render 'shared/navbar' %>

<table class="table">
    <thead class="thead-light">
        <tr>
            <th>Title</th>
            <th>Created At</th>
            <th>Image Url</th>
            <th>Content</th>
        </tr>
    </thead>
    <% @posts.each do |post| %>
        <tr>
            <td><%= post.title %> </td>
            <td><%= post.created_at %> </td>
            <td><%= post.image_url %> </td>
            <td><%= post.content %> </td>
        </tr>
    <% end %>
</table>