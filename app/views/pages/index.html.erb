<div class="container my-4">
    <h1>Posts</h1>
    <table class="table">
        <tr>
            <th>Title</th>
            <th>Created At</th>
            <th>Image Url</th>
            <th>Content</th>
        </tr>
        <% @posts.each do |post| %>
            <tr>
                <td><%= post.title %> </td>
                <td><%= post.created_at %> </td>
                <td><%= post.image_url %> </td>
                <td><%= post.content %> </td>
            </tr>
        <% end %>
    </table>
</div>
